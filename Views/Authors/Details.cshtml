@model muhammedkayraozkaya_241103046.Models.AuthorModel
@{
	var author = Model ?? new AuthorModel(-1);
}

<div class="author-page">
	<div class="books">
		@foreach(var book in author.Books)
		{
			<partial name="Book/preview" model=@book />
		}
	</div>
	<partial name="author-details" model=@author />
</div>

