@{
    ViewData["Title"] = "Home Page";
}

<div class="books">
    @foreach (var book in EnumerableBooks.MostRating.Take(5))
    {
        <div class="book">
            <h4>@book.Title</h4>
            <p>@book.Description</p>
            <p>@book.Rating.ToString("F1")</p>
            <p>@book.StockCount</p>
        </div>
    }
</div>
